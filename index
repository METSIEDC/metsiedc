<!-- Required Swiper CSS and JS -->
<link rel="stylesheet" href="https://unpkg.com/swiper/swiper-bundle.min.css" />
<script src="https://unpkg.com/swiper/swiper-bundle.min.js"></script>

<script>
    document.addEventListener('DOMContentLoaded', function () {
        new Swiper('.mySwiper', {
            effect: 'coverflow', // Enables the 3D coverflow effect
            grabCursor: true,   // Changes cursor to grabbing hand
            centeredSlides: true, // Centers the active slide
            slidesPerView: 'auto', // Adjusts slides per view based on container
            coverflowEffect: {
                rotate: 50,     // Slide rotation in degrees
                stretch: 0,     // Space between slides in px
                depth: 100,     // Depth offset of the slides (z-axis)
                modifier: 1,    // Effect multiplier
                slideShadows: true, // Enables shadows on slides
            },
            loop: true,         // Loop through slides
            autoplay: {
                delay: 3000,      // Autoplay delay in ms (3 seconds)
                disableOnInteraction: false, // Continue autoplay after user interaction
            },
            pagination: {
                el: '.swiper-pagination',
                clickable: true,
            },
            navigation: {
                nextEl: '.swiper-button-next',
                prevEl: '.swiper-button-prev',
            },
            // Responsive breakpoints - adjust slidesPerView for smaller screens
            breakpoints: {
                640: { // Tailwind's 'sm' breakpoint
                    slidesPerView: 1,
                },
                768: { // Tailwind's 'md' breakpoint
                    slidesPerView: 2, // Show 2 slides on medium screens
                },
                1024: { // Tailwind's 'lg' breakpoint
                    slidesPerView: 3, // Show 3 slides on large screens (main effect)
                },
            },
        });
    });
</script>

